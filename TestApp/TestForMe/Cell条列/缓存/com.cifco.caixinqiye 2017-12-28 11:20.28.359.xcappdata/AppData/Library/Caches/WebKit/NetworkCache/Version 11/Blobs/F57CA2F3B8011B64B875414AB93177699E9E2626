/*base.css start*/
html,body{
	font-family: "Microsoft YaHei", "微软雅黑", "宋体", Arial, Helvetica, sans-serif;
}
body{
	-webkit-touch-callout: none;  /*禁止用户长按出现系统菜单栏*/
	-webkit-user-select:none      /*禁止用户选择粘贴*/
}
html,body,div,ul,ol,li,dl,dt,dd,h1,h2,h3,h4,h5,p,input,article{
	padding: 0;
	margin: 0;
}
